{
  "name": "vscode-go-to-mdn",
  "displayName": "vscode-go-to-mdn",
  "description": "Easy way to browse mdn directly from your Visual Studio Code",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.38.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.helloWorld"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.7.5",
    "@types/node-fetch": "^2.5.0",
    "@types/vscode": "^1.38.0",
    "glob": "^7.1.4",
    "mocha": "^6.2.0",
    "tslint": "^5.20.0",
    "typescript": "^3.6.3",
    "vscode-test": "^1.2.0"
  },
  "dependencies": {
    "node-fetch": "^2.6.0",
    "open": "^6.4.0"
  }
}
